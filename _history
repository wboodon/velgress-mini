  }\n        switch (axis) {\n            case Axis.Y:\n                if (player.up.isPressed())\n                    return player.down.isPressed() ? 0 : -1\n                else\n                    return player.down.isPressed() ? 1 : 0\n                break\n            case Axis.X:\n                if (player.left.isPressed())\n                    return player.right.isPressed() ? 0 : -1\n                else\n                    return player.right.isPressed() ? 1 : 0\n                break\n        }\n        return 0\n    }\n\n    export function getXAxis(player: number): number {\n        if (player < 1 || player > 2)\n            return 0\n        return getAxis(Axis.X, player == 1 ? player1 : player2)\n    }\n}"}]},{"timestamp":1765150818658,"editorVersion":"2.0.62","changes":[{"type":"edited","filename":"main.blocks","patch":[{"start1":0,"length1":9140,"diffs":[[1,"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable type=\"KIND_SpriteKind\" id=\"Fq_7uyATnh58Nk-:x=-r\">CameraTarget</variable><variable type=\"KIND_SpriteKind\" id=\"5OGVhZ;T6P{jidr]I.iX\">Player</variable><variable type=\"KIND_SpriteKind\" id=\"@3XN$ci3FJWXYf;m~g}?\">Projectile</variable><variable type=\"KIND_SpriteKind\" id=\"BS:ecPb(f({@{f#5R;9`\">Food</variable><variable type=\"KIND_SpriteKind\" id=\"6,(..YtS#,[TX3ve=Q[D\">Enemy</variable><variable id=\"!zbXlqz@1ZszHtRaWg+K\">playerSprite</variable><variable id=\"`SbyHlHXKF/@On@Xg2%a\">cameraTarget</variable><variable id=\"i3iUQyiS;fRQ]#esZ[l%\">tilemap1</variable><variable id=\"4%Q0/G|w$|b@nvk(7cQ!\">location</variable><variable id=\"iC,IqAxS?;!|ZO]V2[~P\">mySprite</variable></variables><block type=\"gameupdate\" x=\"334\" y=\"-438\"><statement name=\"HANDLER\"><block type=\"controls_if\"><value name=\"IF0\"><shadow type=\"logic_boolean\"><field name=\"BOOL\">TRUE</field></shadow><block type=\"logic_compare\"><field name=\"OP\">LT</field><value name=\"A\"><shadow type=\"math_number\"><field name=\"NUM\">0</f